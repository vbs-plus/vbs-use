{
  "name": "vbs-use",
  "type": "module",
  "version": "0.1.0-beta.2",
  "packageManager": "pnpm@7.12.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "yzh990918 <251205668@qq.com>",
  "homepage": "https://github.com/yzh990918/tsup-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yzh990918/tsup-starter.git"
  },
  "bugs": "https://github.com/yzh990918/tsup-starter/issues",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --fix",
    "docs": "pnpm -C docs run dev",
    "docs:build": "pnpm -C docs run build",
    "prepublishOnly": "zr build",
    "deploy": "pnpm run build && pnpm run docs:build",
    "release": "bumpp && npm publish --access public",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@evan-yang/eslint-config": "^1.0.0",
    "@iconify-json/carbon": "^1.1.9",
    "@iconify-json/fluent-emoji": "^1.1.3",
    "@iconify-json/logos": "^1.1.16",
    "@types/node": "^18.7.18",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "esno": "^0.16.3",
    "fs-extra": "^10.1.0",
    "lint-staged": "^13.0.3",
    "rimraf": "^3.0.2",
    "za-zi": "latest",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.4.0",
    "typescript": "^4.8.3",
    "unocss": "^0.46.4",
    "unplugin-auto-import": "0.11.4",
    "vbs-use": "workspace:*",
    "vite": "^3.1.2",
    "vitepress": "^1.0.0-alpha.28",
    "vitest": "^0.23.4",
    "vue": "^3.2.45"
  },
  "pnpm": {
    "overrides": {
      "vue-demi": "0.13.11",
      "vite": "^3.1.6"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
